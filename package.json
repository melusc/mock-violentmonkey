{
	"author": "lusc",
	"ava": {
		"files": [
			"test/**/*.test.ts"
		],
		"typescript": {
			"compile": false,
			"rewritePaths": {
				"test/": "test-dist/test/"
			}
		}
	},
	"bugs": {
		"url": "https://github.com/melusc/mock-violentmonkey/issues"
	},
	"description": "A way to mock violentmonkey's globals for testing",
	"devDependencies": {
		"@ava/typescript": "^2.0.0",
		"@sindresorhus/tsconfig": "^2.0.0",
		"@types/node": "^16.10.0",
		"@types/tampermonkey": "^4.0.5",
		"ava": "^3.15.0",
		"del-cli": "^4.0.1",
		"prettier": "^2.4.1",
		"typescript": "^4.4.3",
		"xo": "^0.44.0"
	},
	"engines": {
		"node": "^12.20.0 || ^14.13.1 || >=16.0.0"
	},
	"main": "./dist/index.js",
	"files": [
		"dist"
	],
	"homepage": "https://github.com/melusc/mock-violentmonkey#readme",
	"keywords": [
		"violentmonkey",
		"testing",
		"test",
		"mock",
		"userscript"
	],
	"license": "MIT",
	"name": "mock-violentmonkey",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/melusc/mock-violentmonkey.git"
	},
	"scripts": {
		"ava": "ava",
		"prebuild": "npm run lint",
		"build": "del-cli dist/* && tsc",
		"preava": "del-cli test-dist/* && tsc --project test.tsconfig.json",
		"lint": "prettier . -w && xo --fix",
		"test": "xo && npm run ava"
	},
	"version": "0.0.0",
	"dependencies": {
		"type-fest": "^2.3.4"
	}
}
