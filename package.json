		"ava": "^3.15.0",
		"del-cli": "^4.0.1",
		"prettier": "^2.4.1",
		"typescript": "^4.4.4",
		"xo": "^0.46.3"
	},
	"peerDependencies": {
		"ava": "^3.0.0"
	},
